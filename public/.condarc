aggressive_update_packages:
  - ca-certificates
  - certifi
  - openssl
auto_update_conda: true
channels:
  - conda-forge
  - defaults
channel_priority: strict
create_default_packages:
  - pip
pip_interop_enabled: true
always_yes: true
env_prompt: ''
default_threads: 6
ssl_verify: true
auto_activate_base: false
